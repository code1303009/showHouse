<!--index.wxml-->
<view class="container">
  <view wx:for="{{listArray}}" wx:for_item="item.pic"  class='photolist-cell' bindtap='gotoPhotoDetail' id="{{item.id}}">
        <image class='photoImage-cell' src="{{item.pic}}"> </image>
        <label class='title'>{{item.title}}</label>
        <label class='message'>{{item.message}}</label>
  </view>
</view>