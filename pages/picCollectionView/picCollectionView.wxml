<scroll-view class='picCollection' scroll-y>

  <image class="pic-header-image" src="{{picHeader}}" mode='aspectFill' bindtap="lookBigHeader"></image>
  <image class="pic-sub-image" wx:for="{{picList}}" wx:key="*this" src="{{item}}" id="{{item}}" bindtap="lookBigPic" mode='aspectFill'></image>

</scroll-view>